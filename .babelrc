{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "stage-0",
    "react"  
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-es2015-template-literals",
        "transform-runtime",
        "babel-plugin-inject"
      ],
      "presets": [
        "modern-browsers",
        "react-optimize",
        "stage-0",
        "react"
      ]
    },
    "test": {
      "plugins": [
        "istanbul",
        "rewire"
      ],
      "presets": [
        "stage-0",
        "react"
      ]
    }
  }
}