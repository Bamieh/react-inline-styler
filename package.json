{
  "name": "react-inline-styler",
  "version": "1.1.0",
  "description": "react inline styles injector",
  "main": "./distribution/index.js",
  "engines": {
    "node": ">=6.5",
    "npm": ">=3.10"
  },
  "author": "Ahmad Bamieh",
  "license": "MIT",
  "dependencies": {
    "react": "^15.3.2"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-node5": "^11.1.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "coveralls": "^2.13.1",
    "cross-env": "^4.0.0",
    "enzyme": "^2.8.2",
    "glob": "^7.1.1",
    "jsdom": "^9.12.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.3.0",
    "nyc": "^10.2.0",
    "object.entries": "^1.0.4",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.4.2",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-0",
      "react"
    ],
    "env": {
      "production": {
        "presets": ["react-optimize"]
      },
      "test": {
        "plugins": [
          "istanbul",
          "rewire"
        ]
      }
    }
  },
  "scripts": {
    "test": "mocha",
    "test:watch": "yarn run test -- --reporter min --watch",
    "test:cover": "cross-env NODE_ENV=test nyc npm run test",
    "coverage": "yarn run test:cover && nyc report --reporter=html --reporter=text-lcov",
    "clean": "babel-node tools/run clean",
    "build": "babel-node tools/run build",
    "prepublish": "yarn run build",
    "coveralls": "nyc yarn test && nyc report --reporter=text-lcov | coveralls"
  }
}
